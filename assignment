def calculate_average_marks(marks):
    average_marks = dict()
    for name in marks:
    average_marks(name) = sum(marks{name})/len(marks{name})
    return list(average_marks.values())
   marks = {'ismail': {50,74, 60, 67, 90}, 'john': {57, 60, 92, 74, 46}, 'joy': {45, 60, 50, 64, 100}
   calculate_average_marks(marks)







   calculate_average_marks(marks)